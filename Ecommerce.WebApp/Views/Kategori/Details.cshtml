@model IEnumerable<Ecommerce.WebApp.Datas.Entities.KategoriProduk>


@{
    ViewData["Title"] = "Create";
}


<div class="li-main-blog-page pt-60 pb-55">
                <div class="container">
                    <div class="row">
                        <!-- Begin Li's Main Content Area -->
                        <div class="col-lg-12">
                            <div class="row li-main-content">
                                <div class="col-lg-12">
                                    <div class="li-blog-single-item pb-30">
                                        <div class="row">
                                            @foreach (var item in Model)
                                            {
                                                
                                            
                                            <div class="col-lg-6">
                                                <div class="li-blog-banner">
                                                    <a href="blog-details-left-sidebar.html"><img class="img-full" src="~/ext-assets/images/blog-banner/2.jpg" alt=""></a>
                                                </div>
                                            </div>
                                            <div class="col-lg-6">
                                                <div class="li-blog-content">
                                                    <div class="li-blog-details">
                                                        <h3 class="li-blog-heading pt-xs-25 pt-sm-25"><a href="blog-details-left-sidebar.html">@item.NamaKategori</a></h3>
                                                        <p>Deskripsi</p><br>
                                                        <p>@item.Deskripsi</p>
                                                        
                                                    </div>
                                                </div>
                                            </div>
                                            }
                                        </div>
                                            
                                    </div>
                                </div>